    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/flat": "^5.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.179",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.21",
    "@types/qrcode": "^1.4.2",
    "@types/sharp": "^0.29.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.7.1",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --parser flow --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser babel-ts --write"
    ]
  }
}
