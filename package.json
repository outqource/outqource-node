  },
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "@types/stompjs": "^2.3.5",
    "ajv": "^8.11.0",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.0",
    "events": "^3.3.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "firebase-admin": "^10.0.2",
    "flat": "^5.0.2",
    "form-data": "^4.0.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.4",
    "qrcode": "^1.5.0",
    "query-string": "^7.1.1",
    "redis": "^4.0.4",
    "sharp": "^0.30.1",
    "socket.io": "^4.4.1",
